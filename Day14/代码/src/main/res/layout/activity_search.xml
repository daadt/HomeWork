<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.SearchActivity">
    <ImageView
        android:id="@+id/common_share_qq"
        android:src="@drawable/common_back_icon"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="359dp"
        android:layout_marginTop="11dp"
        android:layout_width="35dp"
        android:layout_height="36dp"
        />
    <EditText
        android:id="@+id/common_share_ed"
        android:src="@mipmap/common_ic_filter_down"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"

        android:layout_marginLeft="300dp"
        android:layout_marginRight="309dp"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_width="300dp"
        android:layout_height="50dp"
        />

    <TextView
        android:id="@+id/common_share_te"
        android:text="收索"
        android:layout_width="40dp"
        android:layout_height="36dp"
        android:layout_marginLeft="400dp"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginRight="309dp"
        app:layout_constraintHorizontal_bias="0.106"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/common_record"
        android:layout_width="40dp"
        android:layout_height="36dp"

        android:layout_marginTop="64dp"
        android:layout_marginRight="309dp"
        android:text="收索记录"
        app:layout_constraintHorizontal_bias="0.564"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:layout_margin="@dimen/x90"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>